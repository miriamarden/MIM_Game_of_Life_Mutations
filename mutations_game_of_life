function [] = mutations_game_of_life()


current_gen = randi([0,1],100,100,3);

red_channel = current_gen(:,:,1);
green_channel = current_gen(:,:,2);
blue_channel = current_gen(:,:,3);

mutation_location = current_gen(1,1)

red_channel(mutation_location) = 1;
green_channel(mutation_location) = 0;
blue_channel(mutation_location) = 0;


imagesc(current_gen);


a=1;
while a==1
    [new_gen] = mutations_check_neighbors(current_gen);
    current_gen = new_gen;
    a=a+1;
end



end
